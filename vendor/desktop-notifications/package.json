{
  "name": "desktop-notifications",
  "version": "0.2.5",
  "description": "A simple and opinionated library for handling Windows notifications",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "install": "node-gyp rebuild && tsc",
    "build": "node-gyp rebuild && tsc",
    "pretest": "yarn build",
    "test": "jest -c jest.json",
    "prettify": "yarn prettier --write \"./**/*.{ts,tsx,js,json,jsx,scss,html,yaml,yml}\"",
    "check-prettier": "prettier --check \"./**/*.{ts,tsx,js,json,jsx,scss,html,yaml,yml}\""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/jest": "^26.0.13",
    "@types/node": "^12.0.0",
    "@types/uuid": "^8.3.4",
    "benchmark": "^2.1.4",
    "jest": "^26.4.2",
    "node-gyp": "^10.1.0",
    "prettier": "^2.0.5",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.0"
  },
  "dependencies": {
    "node-addon-api": "^7.0.0",
    "uuid": "^8.3.2"
  },
  "binary": {
    "napi_versions": [
      4
    ]
  },
  "config": {
    "runtime": "napi",
    "target": 4
  }
}
